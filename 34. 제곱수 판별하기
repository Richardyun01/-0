#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
#include <math.h>

int solution(int n) {
    int answer = 0;
    
    for (int i = 1; i <= pow(n, 0.5); i++) {
        if (i * i == n) {
            answer =  1;
        } else {
            answer = 2;
        }
    }
    
    return answer;
}
