#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int limit(int num1, int num2)
{
    if(num1 < 0 || num2 < 0)
        printf("error\n");
    else if(num1 > 100 || num2 > 100)
        printf("error\n");
    else
        return 1;
}

int solution(int num1, int num2) {
    int answer = -1;
    answer = num1 % num2;
    return answer;
}
