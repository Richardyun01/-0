#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

// numbers_len은 배열 numbers의 길이입니다.
double solution(int numbers[], size_t numbers_len) {
    double answer = 0;
    int sum = 0;
    int size = numbers_len;
    
    for (int i = 0; i < size; i++)
        sum += numbers[i];
    
    answer = (double)sum / size;
    
    return answer;
}
