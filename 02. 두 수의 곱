#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num1, int num2) {
    int answer = 0;
    answer = num1 * num2;
    
    return answer;
}

int main()
{
    int num1, num2;
    
    printf("두 수를 입력하시오. \n");
    scanf("%d, %d", &num1, &num2);
    
    if (num1 < 0 || num2 < 0) {
        printf("값이 잘못됬습니다.\n");
    }
    
    else if (num1 > 100 || num2 >100)  {
        printf("값이 잘못됬습니다.\n");
    }
    
    else 
         printf("답은 %d. \n", solution(num1, num2));
           
    return 0;
}
