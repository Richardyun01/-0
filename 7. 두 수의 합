#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int limit(int num1, int num2)
{
    if(num1 < -50000 || num2 < -50000)
        printf("error\n");
    else if(num1 > 50000 || num2 > 50000)
        printf("error\n");
    else
        return 1;
}

int solution(int num1, int num2) {
    int answer = -1;
    answer = num1 + num2;
    return answer;
}
